{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "consistent-return": "off",
    "quotes": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "(^req$|^res$|^next$)" }]
  }
}
